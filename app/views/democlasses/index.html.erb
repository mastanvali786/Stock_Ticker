<h1>Listing democlasses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @democlasses.each do |democlass| %>
      <tr>
        <td><%= democlass.name %></td>
        <td><%= democlass.email %></td>
        <td><%= link_to 'Show', democlass %></td>
        <td><%= link_to 'Edit', edit_democlass_path(democlass) %></td>
        <td><%= link_to 'Destroy', democlass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Democlass', new_democlass_path %>
